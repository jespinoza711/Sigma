@Html.Partial("_Circulos")
<script src="~/Scripts/Vendor/jquery-1.11.1.min.js"></script>
<script src="/Scripts/Vendor/signature_pad.js"></script>
<script src="/Scripts/Vendor/draw-car.js"></script>
<script src="/Scripts/Vendor/app.js"></script>
<script>
    ruta = @Html.Raw("'" + @Model.RutaFirma + "'");
    signaturePad.fromDataURL(ruta);


    @foreach (var coordenada in @Model.Coordenadas)
    {
        <text>
    circulos.push({
        "PointLeft" : @Convert.ToInt32(coordenada.PointLeft),
        "PointBottom" : @Convert.ToInt32(coordenada.PointBottom),
        "PointTop" : @Convert.ToInt32(coordenada.PointTop),
        "PointRight" : @Convert.ToInt32(coordenada.PointRight),
        "EstadoAutoparte": @Html.Raw("'" + coordenada.EstadoAutoparte + "'"),
        "Comentario": @Html.Raw("'"+ coordenada.Comentario + "'"),
        "Orden" : @coordenada.Orden

        });</text>
    }
    dibujarCirculos(circulos);

    @foreach (var coordenadagasolina in @Model.CoordenadasGasolina)
    {
        <text>
    circulosgasolina.push({
        "PointLeft" : @Convert.ToInt32(coordenadagasolina.PointLeft),
        "PointBottom" : @Convert.ToInt32(coordenadagasolina.PointBottom),
        "PointTop" : @Convert.ToInt32(coordenadagasolina.PointTop),
        "PointRight" : @Convert.ToInt32(coordenadagasolina.PointRight),
        "EstadoAutoparte": @Html.Raw("'" + coordenadagasolina.EstadoAutoparte + "'"),
        "Comentario": @Html.Raw("'"+ coordenadagasolina.Comentario + "'"),
        "Orden" : @coordenadagasolina.Orden

        });</text>
    }
    
    dibujarCirculosGasolina(circulosgasolina);

</script>

